<?xml version="1.0" encoding="utf-8"?><resources>    <style name="CustomShapeAppearance">        <item name="neumorph_cornerFamily">oval</item>        <item name="neumorph_cornerSize">20dp</item>        <!-- Or if wants different radii depending on the corner. -->        <item name="neumorph_cornerSizeTopLeft">20dp</item>        <item name="neumorph_cornerSizeTopRight">20dp</item>        <item name="neumorph_cornerSizeBottomLeft">20dp</item>        <item name="neumorph_cornerSizeBottomRight">20dp</item>        <item name="android:windowContentTransitions">true</item>    </style>    <style name="CustomMyMessage">        <item name="neumorph_cornerFamily">rounded</item>        <item name="neumorph_cornerSize">10dp</item>        <!-- Or if wants different radii depending on the corner. -->        <item name="neumorph_cornerSizeTopLeft">20dp</item>        <item name="neumorph_cornerSizeTopRight">20dp</item>        <item name="neumorph_cornerSizeBottomLeft">10dp</item>        <item name="neumorph_cornerSizeBottomRight">0dp</item>        <item name="android:windowContentTransitions">true</item>    </style>    <style name="friend_message">        <item name="neumorph_cornerFamily">rounded</item>        <item name="neumorph_cornerSize">10dp</item>        <!-- Or if wants different radii depending on the corner. -->        <item name="neumorph_cornerSizeTopLeft">10dp</item>        <item name="neumorph_cornerSizeTopRight">10dp</item>        <item name="neumorph_cornerSizeBottomLeft">0dp</item>        <item name="neumorph_cornerSizeBottomRight">20dp</item>        <item name="android:windowContentTransitions">true</item>    </style></resources>